{
   "name": "neuro-store",
   "version": "1.0.5",
   "description": "neuro-store",
   "main": "dist/index.js",
   "module": "dist/index.es.js",
   "types": "dist/index.d.ts",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
